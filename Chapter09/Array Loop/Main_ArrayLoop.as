package {	import flash.display.MovieClip;	public class Main_ArrayLoop extends MovieClip	{		//declare array		private var _collectionJar:Array;				public function Main_ArrayLoop()		{			//Instantiate array			_collectionJar = new Array();						//Add elements to array			_collectionJar.push("fly");			_collectionJar.push("mosquito");			_collectionJar.push("bee");						//Trace entire array contents			for (var i:int = 0; i < _collectionJar.length; i++)			{				trace("Element " + i + ": " + _collectionJar[i]);			}		}	}}