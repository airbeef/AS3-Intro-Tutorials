package {	import flash.display.MovieClip;	public class ShapeMaker	{		public function ShapeMaker()		{		}		static public function makeShape(shape:String):MovieClip		{			switch (shape)			{				case "star" :					return new Star();					break;				case "circle" :					return new Circle();					break;				case "square" :					return new Square();					break;				default :					return new Circle();			}		}	}}