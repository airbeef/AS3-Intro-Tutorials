package {	import flash.display.MovieClip;	import flash.events.Event;	public class Main extends MovieClip	{		public function Main()		{			init();		}		private function init():void		{			addEventListener(Event.ENTER_FRAME, onEnterFrame);		}		private function onEnterFrame(event:Event):void		{			//Player vs wall collision			for (var i:int = 0; i <= 3; i++)			{				CollisionB.rectangleAndRectangle(player, this["wall" + i], 0.2, 0);				//trace("wall" + i);			}		}	}}